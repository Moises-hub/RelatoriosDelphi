  object SQLQuery1: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'select'
      'pedidos.data_hora,'
      'funcionarios.nome as vendedor,'
      'pedidos.id_pedido,'
      'pedidos.id_cliente,'
      'pedidos.NOTA_FISCAL,'
      'produtos.descricao,'
      'produtos.codigo_NCM,'
      'pedido_itens.qtde,'
      'pedido_itens.valor_unitario,'
      'pedido_itens.valor_final_un,'
      'produto_precos.preco_tabela as preco_tabela,'
      
        'pedido_itens.valor_final_un - produto_precos.preco_tabela as dif' +
        'erenca_un,'
      
        '(pedido_itens.qtde * pedido_itens.valor_final_un) - (produto_pre' +
        'cos.preco_tabela * pedido_itens.qtde) as diferenca,'
      
        '(pedido_itens.qtde * pedido_itens.valor_final_un) as valor_total' +
        ' ,'
      'clientes.nome,'
      'regioes.cidade || '#39'-'#39' || regioes.uf as cidade'
      'from pedidos'
      'left join clientes on (clientes.id_cliente = pedidos.id_cliente)'
      
        'left join funcionarios on (funcionarios.id_funcionario = pedidos' +
        '.id_vendedor)'
      'left join regioes  on (regioes.id_regiao = clientes.id_regiao)'
      
        'left join pedido_itens on (pedido_itens.id_pedido = pedidos.id_p' +
        'edido)'
      
        'left join produtos on (produtos.id_produto = pedido_itens.id_pro' +
        'duto)'
      
        'left join natureza_operacao on (natureza_operacao.id_natureza = ' +
        'pedidos.id_natureza)'
      
        'left join produto_precos on (produto_precos.id_produto = produto' +
        's.id_produto and produto_precos.ativo = 1)'
      'ORDER BY vendedor')
    SQLConnection = DM.SQLConnection1
    Left = 728
    Top = 48
